<Window x:Class="WpfModelFirst.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfModelFirst"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Menu>
            <MenuItem Header="Adatok kezelése">
                <MenuItem x:Name="menuitemUsers" Header="Felhasználók" Click="menuitemUsers_Click"/>
                <MenuItem x:Name="menuitemPosts" Header="Posztok" Click="menuitemPosts_Click"/>
            </MenuItem>
        </Menu>

    </Grid>
</Window>
